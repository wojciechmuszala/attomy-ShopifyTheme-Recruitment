<li
  class="faq__group-list"
  data-type="faq-group"
  data-group-name="{{ block.settings.name }}"
  data-collapse-close-rest
  {{ block.shopify_attributes }}
>
  <div class="faq__group-item">
    {% content_for 'blocks' %}
  </div>
</li>

{% schema %}
{
  "name": "FAQ group",
  "tag": "ul",
  "class": "faq__group",
  "settings": [
    {
      "type": "text",
      "label": "Group name",
      "id": "name"
    }
  ],
  "blocks": [
    {
      "type": "_faq-list"
    }
  ],
  "presets": [
    {
      "name": "FAQ group"
    }
  ]
}
{% endschema %}
